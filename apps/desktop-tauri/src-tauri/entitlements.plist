<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Allow JIT compilation for WebView -->
    <key>com.apple.security.cs.allow-jit</key>
    <true/>

    <!-- Allow unsigned executable memory (required for some features) -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <true/>

    <!-- Allow loading of third-party libraries -->
    <key>com.apple.security.cs.disable-library-validation</key>
    <true/>

    <!-- Camera access for video calls -->
    <key>com.apple.security.device.camera</key>
    <true/>

    <!-- Microphone access for audio -->
    <key>com.apple.security.device.audio-input</key>
    <true/>

    <!-- Screen capture for screen sharing -->
    <key>com.apple.security.cs.allow-dyld-environment-variables</key>
    <true/>

    <!-- Network client access -->
    <key>com.apple.security.network.client</key>
    <true/>

    <!-- Keychain access for secure storage -->
    <key>com.apple.security.keychain</key>
    <true/>
</dict>
</plist>
